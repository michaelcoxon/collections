{
  "name": "@michaelcoxon/collections",
  "version": "2.0.0-alpha.1",
  "description": "Collections for JS and TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "run-tests": "mocha -r ts-node/register tests/**/*.spec.ts",
    "test": "npm-run-all -s build-prod -p run-tests",
    "build-dev": "SET NODE_ENV=development&& webpack --no-color",
    "build-prod": "SET NODE_ENV=production&& webpack --no-color",
    "build-doc": "typedoc --out docs src&& copy .nojekyll docs\\.nojekyll",
    "prepack": "npm-run-all -s test build-doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelcoxon/collections.git"
  },
  "keywords": [
    "collections",
    "typescript"
  ],
  "author": "Michael Coxon",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/michaelcoxon/collections/issues"
  },
  "homepage": "https://github.com/michaelcoxon/collections#readme",
  "devDependencies": {
    "@types/chai": "4.1.6",
    "@types/mocha": "5.2.5",
    "@types/prop-types": "15.5.6",
    "@types/webpack-env": "1.13.6",
    "awesome-typescript-loader": "5.2.1",
    "chai": "4.2.0",
    "dts-bundle": "0.7.3",
    "file-loader": "2.0.0",
    "json-loader": "0.5.7",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.3",
    "ts-node": "7.0.1",
    "typedoc": "0.13.0",
    "typescript": "3.1.3",
    "uglifyjs-webpack-plugin": "2.0.1",
    "webpack": "4.20.2",
    "webpack-log": "2.0.0",
    "webpack-cli": "3.1.2",
    "webpack-cyclic-dependency-checker": "0.0.1"
  },
  "dependencies": {
    "tslib": "1.9.3",
    "@michaelcoxon/utilities": "1.7.5"
  }
}
