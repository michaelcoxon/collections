{
  "name": "@michaelcoxon/collections",
  "version": "2.0.0-alpha",
  "description": "Collections for JS and TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "run-tests": "mocha -r ts-node/register tests/**/*.spec.ts",
    "test": "npm-run-all -s build-dev build-prod -p run-tests",
    "build-dev": "SET NODE_ENV=development&& webpack --no-color",
    "build-prod": "SET NODE_ENV=production&& webpack --no-color",
    "prepack": "npm-run-all -s test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelcoxon/collections.git"
  },
  "keywords": [
    "collections",
    "typescript"
  ],
  "author": "Michael Coxon",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/michaelcoxon/collections/issues"
  },
  "homepage": "https://github.com/michaelcoxon/collections#readme",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.2",
    "@types/prop-types": "^15.5.3",
    "@types/webpack-env": "^1.13.6",
    "awesome-typescript-loader": "^5.0.0",
    "chai": "^4.1.2",
    "dts-bundle": "^0.7.3",
    "file-loader": "^1.1.11",
    "json-loader": "^0.5.7",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "ts-node": "^6.1.1",
    "typescript": "^2.9.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@michaelcoxon/utilities": "^1.4.1"
  }
